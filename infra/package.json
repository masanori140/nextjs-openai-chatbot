{
  "name": "openmetadata-on-amazon-eks",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "apply": "cdktf deploy",
    "build": "tsc",
    "compile": "tsc --pretty",
    "get": "cdktf get",
    "lint:format": "prettier --check --ignore-path .prettierignore .",
    "lintfix:format": "prettier --write --list-different --ignore-path .prettierignore .",
    "plan": "cdktf diff",
    "synth": "cdktf synth",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "watch": "tsc -w"
  },
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "@cdktf/provider-archive": "^10.0.1",
    "@cdktf/provider-aws": "19.16.0",
    "cdktf": "^0.20.7",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.6",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  }
}
